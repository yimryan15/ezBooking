{{#each flightData}}
<div class="results-container">
  <div class="outbound">
    <p>outbound origin city: {{outboundCity}}</p>
    <p>outbound duration: {{outBoundDuration}}</p>
    <p>outbound departure time: {{outDepartTime}}</p>
    <p>outbound arrival time: {{outArriveTime}}</p>
  </div>
  <div class="inbound">
    <p>inbound origin city: {{inboundCity}}</p>
    <p>inbound duration: {{inBoundDuration}}</p>
    <p>inbound departure time: {{inDepartTime}}</p>
    <p>inbound departure time: {{inArriveTime}}</p>
  </div>
  <p>price: ${{price}}</p>
  <a target="_blank" href="{{deepLink}}">Link to Book Flight</a>
</div>
{{/each}}
